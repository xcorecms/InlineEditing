{
  "name": "xcore/inline-editing",
  "type": "library",
  "description": "Content inline editable package.",
  "keywords": [
    "content editable",
    "xcore"
  ],
  "homepage": "https://appio.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "bin": [
    "bin/inline"
  ],
  "require": {
    "php": "^7.4 || ^8.0",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "doctrine/orm": "^2.5",
    "nette/database": "^3.0",
    "dibi/dibi": "^4.0",
    "symfony/property-access": "^5.0",
    "symfony/validator": "^5.0",
    "squizlabs/php_codesniffer": "^3.0",
    "tracy/tracy": "^2.4",
    "nette/tester": "^2.0",
    "phpstan/phpstan": "^0.12",
    "mockery/mockery": "^1.1"
  },
  "suggest": {
    "doctrine/dbal": "If you wanna use DBAL persistence layer",
    "nette/database": "If you wanna use Nette Database persistence layer",
    "dibi/dibi": "If you wanna use Dibi persistence layer",
    "doctrine/orm": "If you wanna use entity features",
    "symfony/validator": "If you wanna use entity validation features",
    "symfony/property-access": "If you wanna use entity features"
  },
  "autoload": {
    "psr-4": {
      "XcoreCMS\\InlineEditing\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "XcoreCMS\\InlineEditing\\Tests\\": "tests/"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.1-dev"
    }
  }
}
